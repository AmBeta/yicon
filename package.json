{
  "name": "yicon",
  "version": "1.0.0",
  "description": "Font generation platform",
  "main": "bin/server.js",
  "scripts": {
    "lint": "eslint --ext .js,.jsx src",
    "start": "webpack --progress && qxf dev",
    "start:dev": "better-npm-run start:dev",
    "client": "better-npm-run client",
    "dev": "concurrently --kill-others \"npm run client\" \"npm run start:dev\"",
    "sync": "SEQUELIZE_SYNC=SYNC qxf dev",
    "force": "SEQUELIZE_SYNC=SYNC_FORCE qxf dev",
    "test": "karma start"
  },
  "betterScripts": {
    "client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "start:dev": {
      "command": "node ./node_modules/nodemon/bin/nodemon.js ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YMFE/yicon.git"
  },
  "keywords": [
    "react",
    "iconfont",
    "universal",
    "webpack",
    "express",
    "redux"
  ],
  "author": "malcolmyu <minghao_yu@163.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YMFE/yicon/issues"
  },
  "engines": {
    "node": "~5.x",
    "npm": "~3.x"
  },
  "homepage": "https://github.com/YMFE/yicon#readme",
  "dependencies": {
    "axios": "^0.11.1",
    "babel": "^6.5.2",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "css-loader": "^0.23.1",
    "debug": "~2.2.0",
    "history": "^2.1.1",
    "koa": "^1.2.0",
    "koa-body": "^1.4.0",
    "koa-compress": "^1.0.9",
    "koa-favicon": "^1.2.1",
    "koa-logger": "^1.3.0",
    "koa-mount": "^1.3.0",
    "koa-router": "^5.4.0",
    "koa-session-store": "^2.0.0",
    "koa-static": "^2.0.0",
    "material-ui": "^0.15.0",
    "mysql": "^2.10.2",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "process": "^0.11.3",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-engine": "^3.4.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^3.2.0",
    "sequelize": "^3.23.2",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-isomorphic-tools": "^2.3.0"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "better-npm-run": "0.0.8",
    "concurrently": "^2.1.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "koa-webpack-dev-middleware": "^1.2.1",
    "koa-webpack-hot-middleware": "^1.0.3",
    "nodemon": "^1.9.2",
    "pre-commit": "^1.1.3",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11"
  }
}
